; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="ApexSim"
config/description="ApexSim multiplayer racing simulation client"
run/main_scene="res://scenes/loading_screen.tscn"
config/features=PackedStringArray("4.5", "C#")
config/icon="res://assets/logo.png"

[autoload]

SceneManager="*res://scripts/scene_manager.gd"
Network="*res://scripts/csharp/NetworkClient.cs"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="ApexSim"

[rendering]

renderer/rendering_method.mobile="forward_plus"
